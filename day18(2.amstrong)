//program to check wether the number is amstrong or not 
#include<stdio.h>
main()
{
	int num,original,rem,sum=0;
	scanf("%d",&num);
	original=num;
	while(original!=0)
	{
		rem=original%10;
		sum=sum+rem*rem*rem;
		original=original/10;
	}
	if(sum==num)
	printf("%d is a amstrong number",num);
	else
	printf("%d is not a amstrong number",num);
	getch();
 } 
