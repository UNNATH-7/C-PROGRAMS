// wap c program to read 20 members ages and their fallen age group
// 0-5 baby; 6-21 child; 22-45 adult; 46-100 old
//author:UNNATH
#include<stdio.h>
main()
{
	int t,age,baby=0,child=0,adult=0,old=0;
	scanf("%d",&t);
	while(t--)
	{
		scanf("%d",&age);
		if(age>=0&&age<=5)
		{
			baby++;
		}
		if(age>=6&&age<=21)
		{
			child++;
		}
		if(age>=22&&age<=45)
		{
			adult++;
		}
        if(age>=46&&age<=100)
		{
			old++;
		}
	}
	printf("\n the persons in 0-5 age group(baby age) is=%d",baby);
	printf("\n the persons in 6-25 age group(school/college age) is=%d",child);
	printf("\n the persons in 19-45 age group(adult age) is=%d",adult);
	printf("\n the persons in 46-100 age group(old age) is=%d",old);
	getch();	
}
